{
  "name": "draft-js-video-plugin",
  "version": "2.0.1",
  "description": "Video Plugin for DraftJS",
  "author": {
    "name": "Anchen li"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/draft-js-plugins/draft-js-plugins.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "video",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --require testHelper.js **/__test__/*.js",
    "clean": "../node_modules/.bin/rimraf lib",
    "build": "yarn clean && yarn build:js && yarn build:css",
    "build:js": "node ../node_modules/.bin/babel src --out-dir=lib --root-mode=upward --ignore='__test__/*'",
    "build:css": "node ../scripts/build-css.js $(pwd)",
    "prepublish": "yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1",
    "react": "^15.5.0 || ^16.0.0-rc",
    "react-dom": "^15.5.0 || ^16.0.0-rc"
  }
}
